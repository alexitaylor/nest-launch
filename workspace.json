{
  "version": 1,
  "projects": {
    "react": {
      "root": "apps/react",
      "sourceRoot": "apps/react/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react",
            "index": "apps/react/src/index.html",
            "main": "apps/react/src/main.tsx",
            "polyfills": "apps/react/src/polyfills.ts",
            "tsConfig": "apps/react/tsconfig.app.json",
            "assets": ["apps/react/src/favicon.ico", "apps/react/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react/src/environments/environment.ts",
                  "with": "apps/react/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/react/tsconfig.app.json",
              "apps/react/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react/jest.config.js",
            "tsConfig": "apps/react/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-e2e": {
      "root": "apps/react-e2e",
      "sourceRoot": "apps/react-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-e2e/cypress.json",
            "tsConfig": "apps/react-e2e/tsconfig.e2e.json",
            "devServerTarget": "react:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/react-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/react-e2e/**/*"]
          }
        }
      }
    },
    "nest": {
      "root": "apps/nest",
      "sourceRoot": "apps/nest/src",
      "projectType": "application",
      "prefix": "nest",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/nest",
            "main": "apps/nest/src/main.ts",
            "tsConfig": "apps/nest/tsconfig.app.json",
            "assets": ["apps/nest/src/assets"],
            "showCircularDependencies": false
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nest/src/environments/environment.ts",
                  "with": "apps/nest/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "nest:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/nest/tsconfig.app.json",
              "apps/nest/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/nest/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nest/jest.config.js",
            "tsConfig": "apps/nest/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "@emotion/styled"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "react"
}
